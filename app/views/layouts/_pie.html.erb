  <div class="container my-3 border-top bg-dark text-white py-4">
        <% if session[:user_id] %>
        <%  @user = User.find_by(id: session[:user_id]) %>
          <div class="row">
          <div class="col-12 col-md-6">
            <h5><strong><%= @user.name %></strong>, administrador con privilegios "<%= @user.tipo %>"</h5> <%= button_to 'Logout', logout_path, method: :delete, class:"btn btn-danger" %>
          </div>
          <div class="col-12 col-md-6">
            <ul >
            <% if(@user.tipo=="Total") %>
              <li><%= link_to 'Users', users_path, class:"h5" %></li>
            <% end %>
            <% if(@user.tipo=="Total" || @user.tipo=="Blog") %>
              <li><%= link_to 'Post', posts_path, class:"h5" %></li>
            <% end %>
            <% if(@user.tipo=="Total" || @user.tipo=="Publicidad") %>
              <li><%= link_to 'Publicidad', publicidads_path, class:"h5" %></li>
            <% end %>
            </ul>
          </div>
        <% end %>
</div>
